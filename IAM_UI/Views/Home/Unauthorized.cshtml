@{
    ViewData["Title"] = "Unauthorized";
    Layout = "~/Views/Shared/_LayoutUser.cshtml";
}

<!-- Start Content -->
<div class="container-fluid">
    <div class="row justify-content-center">
        <div class="col-lg-6 col-xl-4 mb-4">
            <div class="error-text-box">
                <svg viewBox="0 0 600 200">
                    <!-- Symbol -->
                    <symbol id="s-text">
                        <text text-anchor="middle" x="50%" y="50%" dy=".35em">401!</text>
                    </symbol>
                    <!-- Duplicate symbols -->
                    <use class="text" xlink:href="#s-text"></use>
                    <use class="text" xlink:href="#s-text"></use>
                    <use class="text" xlink:href="#s-text"></use>
                    <use class="text" xlink:href="#s-text"></use>
                    <use class="text" xlink:href="#s-text"></use>
                </svg>
            </div>
            <div class="text-center">
                <h3 class="mt-0 mb-2 text-danger">Access Denied!</h3>
                <p class="text-muted mb-3">
                    You do not have permission to view this page. If you believe this is an error, please contact the administrator.
                </p>

                <a href="/Home/Dashboard/" class="btn btn-success waves-effect waves-light">Back to Dashboard</a>
               
            </div>
        </div> <!-- end col -->
    </div>
    <!-- end row -->
</div> <!-- container -->
